<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.webdev.kpoint.bl.pojo.ReferralSource" table="referralsource">
		<id name="id" type="java.lang.Integer"
			column="id" unsaved-value="-1">
			<generator class="native" />
		</id>
		
		<property name="displayName" type="java.lang.String" />
		
		<property name="displayInAdmin" type="java.lang.Boolean" />
		
		<property name="displayInConsumer" type="java.lang.Boolean" />
		
		<property name="displayIndex" type="java.lang.Integer" />
	</class>
</hibernate-mapping>
