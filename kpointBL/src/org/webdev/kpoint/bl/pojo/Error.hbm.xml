<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.webdev.kpoint.bl.pojo.Error" table="wserror">
		
		<id name="id" type="java.lang.Integer" column="id" unsaved-value="-1">
			<generator class="native" />
		</id>
		
		<property name="errorCode" type="java.lang.String">
			<column name="ErrorCode" />
		</property>

		<property name="friendlyMessage" type="java.lang.String">
			<column name="FriendlyMessage" />
		</property>
		
	</class>
</hibernate-mapping>