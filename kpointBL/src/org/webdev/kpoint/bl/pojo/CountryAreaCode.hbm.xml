<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.webdev.kpoint.bl.pojo.CountryAreaCode" table="countryareacode">
		<id name="countryAreaCodeId" type="java.lang.Integer"
			column="id" unsaved-value="-1">
			<generator class="native" />
		</id>
		
		<property name="areaCode" type="java.lang.String">
			<column name="areaCode" />
		</property>
		
		<property name="countryCode" type="java.lang.String">
			<column name="countryCode" />
		</property>
	</class>
</hibernate-mapping>