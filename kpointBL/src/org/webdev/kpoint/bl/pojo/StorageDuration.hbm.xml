<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.webdev.kpoint.bl.pojo.StorageDuration" table="storageduration">
		<id name="id" type="java.lang.Integer" column="id" unsaved-value="-1">
			<generator class="native" />
		</id>
		
		<property name="minDays" type="java.lang.Integer">
			<column name="minDays" />
		</property>
		
		<property name="maxDays" type="java.lang.Integer">
			<column name="maxDays" />
		</property>
	</class>
</hibernate-mapping>